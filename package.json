    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "sass": "^1.32.4",
    "sass-loader": "^10.1.1",
    "vue-template-compiler": "^2.6.11"
